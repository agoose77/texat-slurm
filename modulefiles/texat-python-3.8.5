proc ModulesHelp { } {
        global version modroot

        puts stderr "texat - sets the Environment for texat Python 3.8.5 in my home directory"
}

module-whatis   "Sets the environment for using texat Python"

# for Tcl script use only
set     home            $env(HOME)
set     topdir          <PATH>/texat-python-3.8.5
set     version         3.8.5
set     sys             linux86

prepend-path    PATH            $topdir/bin
prepend-path    PATH            $topdir/scripts

